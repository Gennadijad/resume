
<div>
<h1>{{name}}</h1>
    <h2>{{> title data="Users"}}</h2>
    {{#each users}}
        <ul>
            <li>id: {{id}}</li>
            <li>name: {{name}}</li>
            <li>gender: {{gender}}</li>
            <li>age: {{age}}</li>
        </ul>    
        <br>
        <h3>{{> title data="Friends"}}</h3>
        {{#each friends}}
            <ul>
                <li>id: {{id}}</li>
                <li>name: {{name}}</li>
                <li>gender: {{gender}}</li>
                <li>age: {{age}}</li>
            </ul>
            <br>
            <h3>{{> title data="Mutual_friends"}}</h3>
            {{#each mutual_friends}}
                <ul>
                    <li>id: {{id}}</li>
                    <li>name: {{name}}</li>
                    <li>gender: {{gender}}</li>
                    <li>age: {{age}}</li>
                </ul>
            {{/each}}
            <br>
            <h3>{{> title data="Groups"}}</h3>
            {{#each groups}}
                <ul>
                    <li>id: {{id}}</li>
                    <li>name: {{name}}</li>
                    <li>description: {{description}}</li>
                    <br>
                    <h3>{{> title data="Members"}}</h3>
                    {{#each members}}
                        <li>id: {{id}}</li>
                        <li>name: {{name}}</li>
                        <li>gender: {{gender}}</li>
                        <li>age: {{age}}</li>
                    {{/each}}
                </ul>
           {{/each}}
        {{/each}}
    {{/each}}
</div>